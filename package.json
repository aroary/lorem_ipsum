{
  "name": "loremipsum",
  "displayName": "lorem ipsum",
  "version": "3.1.0",
  "description": "Add lorem ipsum to your code",
  "icon": "icon.png",
  "publisher": "aroary",
  "main": "index.js",
  "browser": "browser/index.min.js",
  "scripts": {
    "start": "vscode-test-web --extensionDevelopmentPath=. .",
    "package": "vsce package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aroary/lorem_ipsum.git"
  },
  "engines": {
    "vscode": "^1.69.1"
  },
  "activationEvents": [
    "onCommand:lorem_ipsum.text",
    "onCommand:lorem_ipsum.image",
    "onCommand:lorem_ipsum.language"
  ],
  "contributes": {
    "configuration": {
      "title": "lorem_ipsum",
      "properties": {
        "lorem_ipsum.language": {
          "scope": "window",
          "type": "array",
          "default": [
            "lorem",
            "ipsum",
            "dolor",
            "sit",
            "amet",
            "consectetur",
            "adipiscing",
            "elit",
            "sed",
            "do",
            "eiusmod",
            "tempor",
            "incididunt",
            "ut",
            "labore",
            "et",
            "dolore",
            "magna",
            "aliqua",
            "enim",
            "ad",
            "minim",
            "veniam",
            "quis",
            "nostrud",
            "exercitation",
            "ullamco",
            "laboris",
            "nisi",
            "aliquip",
            "ex",
            "ea",
            "commodo",
            "consequat",
            "Duis",
            "aute",
            "irure",
            "in",
            "reprehenderit",
            "voluptate",
            "velit",
            "esse",
            "cillum",
            "eu",
            "fugiat",
            "nulla",
            "pariatur",
            "Excepteur",
            "sint",
            "occaecat",
            "cupidatat",
            "non",
            "proident",
            "sunt",
            "culpa",
            "qui",
            "officia",
            "deserunt",
            "mollit",
            "anim",
            "id",
            "est",
            "laborum"
          ],
          "minItems": 8,
          "maxItems": 200,
          "description": "Custom text to generate."
        }
      }
    },
    "commands": [
      {
        "command": "lorem_ipsum.text",
        "title": "lorem ipsum Generate text",
        "category": "generate",
        "icon": "icon.png"
      },
      {
        "command": "lorem_ipsum.image",
        "title": "lorem ipsum Generate image",
        "category": "generate",
        "icon": "icon.png"
      },
      {
        "command": "lorem_ipsum.language",
        "title": "Download and change lorem ipsum language",
        "icon": "icon.png"
      }
    ]
  },
  "keywords": [
    "lorem",
    "ipsum",
    "text",
    "generator"
  ],
  "author": "aroary",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aroary/lorem_ipsum/issues"
  },
  "homepage": "https://github.com/aroary/lorem_ipsum#readme",
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@vscode/test-web": "^0.0.44"
  },
  "dependencies": {
    "path": "^0.12.7",
    "sharp": "^0.32.3"
  }
}
